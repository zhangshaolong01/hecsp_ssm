    <!--服务项	tb_service_item-->
    <sqlMap resource="config/ibatis/mysql/set/serviceitem/ServiceItem.xml"/>
    <!--	tb_service_package-->
    <sqlMap resource="config/ibatis/mysql/set/serviceitem/ServicePackage.xml"/>
    <!--	tb_service_package_items-->
    <sqlMap resource="config/ibatis/mysql/set/serviceitem/ServicePackageItems.xml"/>
    <!--服务项的类别,从字典移植过来	tb_service_category-->
	<sqlMap resource="config/ibatis/mysql/set/serviceitem/ServiceCategory.xml"/>
</sqlMapConfig>
